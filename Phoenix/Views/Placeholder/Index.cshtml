@{
    ViewBag.Title = "Online RCI Home Page";
    ViewBag.ViewTitle = "Online RCI Landing Page";
}

@section Styles{
    @Styles.Render("~/bundles/page_specific/css/landing_page")
}

@section Scripts{
    @Scripts.Render("~/bundles/page_specific/js/landing_page")
}


<div class="construction-container">
    <div class="image-container">
        <img class="phoenix-construction-image" src="~/Content/Images/phoenix.png" />
    </div>
    <p class="main-header">Coming soon...</p>

    <div id="clock-div">
        <div>
            <span class="days"></span>
            <div class="small-text">Days</div>
        </div>
        <div>
            <span class="hours"></span>
            <div class="small-text">Hours</div>
        </div>
        <div>
            <span class="minutes"></span>
            <div class="small-text">Minutes</div>
        </div>
        <div>
            <span class="seconds"></span>
            <div class="small-text">Seconds</div>
        </div>

    </div>
</div>

